    "express": "^4.17.1",
    "jaeger-client": "^3.17.0",
    "nats": "^1.3.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
